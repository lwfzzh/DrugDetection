<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/white">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/top">
            <Button
                    android:id="@+id/test_results_return_button"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="-5dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/return_button"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="测试结果"
                    android:textColor="#D5D5D6"
                    android:textSize="24dp"
                    android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="520dp"
                android:orientation="vertical">
            <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fillViewport="true"
                    android:scrollbars="none">
                <LinearLayout
                        android:layout_width="360dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:layout_margin="10dp"
                            android:orientation="vertical">
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp">
                            <TextView
                                    android:layout_width="40dp"
                                    android:layout_height="match_parent"
                                    android:text="编号:"
                                    android:textColor="@color/black"
                                    android:textSize="14dp"
                                    android:gravity="center"/>
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="7001050001"
                                    android:textColor="@color/black"
                                    android:textSize="14dp"
                                    android:gravity=""/>
                        </LinearLayout>
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp">
                            <TextView
                                    android:layout_width="40dp"
                                    android:layout_height="match_parent"
                                    android:text="日期:"
                                    android:textColor="@color/black"
                                    android:textSize="14dp"
                                    android:gravity="center"/>
                            <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="2022-10-22"
                                    android:textColor="@color/black"
                                    android:textSize="14dp"
                                    android:gravity=""/>
                        </LinearLayout>
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp">
                            <LinearLayout
                                    android:layout_width="140dp"
                                    android:layout_height="match_parent">
                                <TextView
                                        android:layout_width="40dp"
                                        android:layout_height="match_parent"
                                        android:text="标本:"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity="center"/>
                                <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="毛发"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity=""/>
                            </LinearLayout>
                            <LinearLayout
                                    android:layout_width="200dp"
                                    android:layout_height="30dp">
                                <TextView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:text="用户名:"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity="center"/>
                                <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="admin"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity=""/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp">
                            <LinearLayout
                                    android:layout_width="140dp"
                                    android:layout_height="match_parent">
                                <TextView
                                        android:layout_width="40dp"
                                        android:layout_height="match_parent"
                                        android:text="条码:"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity="center"/>
                                <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="123"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity=""/>
                            </LinearLayout>
                            <LinearLayout
                                    android:layout_width="200dp"
                                    android:layout_height="30dp">
                                <TextView
                                        android:layout_width="40dp"
                                        android:layout_height="match_parent"
                                        android:text="时间:"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity="center"/>
                                <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:text="2022-12-22 18:10:00"
                                        android:textColor="@color/black"
                                        android:textSize="14dp"
                                        android:gravity=""/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="360dp"
                            android:layout_height="180dp"
                            android:orientation="vertical">
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:background="#e7e7e7">
                            <TextView
                                    android:layout_width="60dp"
                                    android:layout_height="40dp"
                                    android:text="个人信息"
                                    android:textSize="14dp"
                                    android:layout_marginLeft="10dp"
                                    android:gravity="center"
                                    android:textColor="#989898"/>
                        </LinearLayout>
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:layout_marginTop="5dp"
                                android:paddingTop="20dp"
                                android:paddingBottom="20dp"
                                android:paddingLeft="15dp"
                                android:background="@color/white"
                                android:orientation="vertical">
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:orientation="horizontal">
                                <TextView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="姓名:"
                                        android:textSize="14dp"/>
                                <TextView
                                        android:layout_width="100dp"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="张三"
                                        android:textSize="14dp"/>
                                <TextView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:text="性别:"
                                        android:textSize="14dp"
                                        android:textColor="@color/black"/>
                                <TextView
                                        android:layout_width="80dp"
                                        android:layout_height="match_parent"
                                        android:text="男"
                                        android:textSize="14dp"
                                        android:textColor="@color/black"/>
                            </LinearLayout>
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:orientation="horizontal">
                                <TextView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:text="民族:"
                                        android:textSize="14dp"
                                        android:textColor="@color/black"/>
                                <TextView
                                        android:layout_width="100dp"
                                        android:layout_height="match_parent"
                                        android:text="汉"
                                        android:textSize="14dp"
                                        android:textColor="@color/black"/>
                                <TextView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="生日:"
                                        android:textSize="14dp"/>
                                <TextView
                                        android:layout_width="100dp"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="2020-09-17"
                                        android:textSize="14dp"/>
                            </LinearLayout>
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:orientation="horizontal">
                                <TextView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="地址:"
                                        android:textSize="14dp"/>
                                <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="广东省广州市黄埔区萝岗街萝岗社区"
                                        android:textSize="14dp"/>
                            </LinearLayout>
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:orientation="horizontal">
                                <TextView
                                        android:layout_width="50dp"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="证件号:"
                                        android:textSize="14dp"/>
                                <TextView
                                        android:layout_width="200dp"
                                        android:layout_height="match_parent"
                                        android:textColor="@color/black"
                                        android:text="400000000000000000"
                                        android:textSize="14dp"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="360dp"
                            android:layout_height="90dp"
                            android:layout_marginTop="10dp"
                            android:orientation="vertical">
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:background="#e7e7e7">
                            <TextView
                                    android:layout_width="60dp"
                                    android:layout_height="40dp"
                                    android:text="检测结果"
                                    android:textSize="14dp"
                                    android:layout_marginLeft="10dp"
                                    android:gravity="center"
                                    android:textColor="#989898"/>
                        </LinearLayout>
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="60dp"
                                android:background="@color/white"
                                android:orientation="vertical">
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="25dp">
                                <TextView
                                        android:layout_width="114dp"
                                        android:layout_height="match_parent"
                                        android:text="项目"
                                        android:textSize="14dp"
                                        android:gravity="center"
                                        android:textColor="@color/black"/>
                                <TextView
                                        android:layout_width="113dp"
                                        android:layout_height="match_parent"
                                        android:text="结果"
                                        android:textSize="14dp"
                                        android:textColor="@color/black"
                                        android:gravity="center"/>
                                <TextView
                                        android:layout_width="113dp"
                                        android:layout_height="match_parent"
                                        android:text="单位"
                                        android:textSize="14dp"
                                        android:textColor="@color/black"
                                        android:gravity="center" />
                            </LinearLayout>
                            <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="25dp">
                                <TextView
                                        android:layout_width="114dp"
                                        android:layout_height="match_parent"
                                        android:text="冰毒"
                                        android:textSize="12dp"
                                        android:gravity="center"
                                        android:textColor="@color/black"/>
                                <TextView
                                        android:layout_width="113dp"
                                        android:layout_height="match_parent"
                                        android:text="1.00"
                                        android:textSize="12dp"
                                        android:textColor="@color/black"
                                        android:gravity="center"/>
                                <TextView
                                        android:layout_width="113dp"
                                        android:layout_height="match_parent"
                                        android:text="mg/ml"
                                        android:textSize="12dp"
                                        android:textColor="@color/black"
                                        android:gravity="center" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </ScrollView>
        </LinearLayout>
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">
            <Button
                    android:layout_width="165dp"
                    android:layout_height="45dp"
                    android:background="@drawable/button_frame_shape"
                    android:layout_marginLeft="10dp"
                    android:text="打印"
                    android:textColor="@color/white"
                    android:textSize="16dp" />
            <Button
                    android:layout_width="165dp"
                    android:layout_height="45dp"
                    android:background="@drawable/button_black"
                    android:layout_marginLeft="10dp"
                    android:text="测试完成"
                    android:textColor="@color/white"
                    android:textSize="16dp" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>